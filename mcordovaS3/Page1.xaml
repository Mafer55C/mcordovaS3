<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mcordovaS3.Page1">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stackLayoutStyle}">
            <Label Text="SEMANA 3" FontSize="18" HorizontalOptions="Center" />
            <Entry x:Name="txtDatoUno" Placeholder="Dato1" />
            <Entry x:Name="txtDatoDos" Placeholder="Dato2" Keyboard="Numeric"/>
            <Button x:Name="btnAbrir" Text="ABRIR" Clicked="btnAbrir_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>